{
  "name": "portfolio",
  "version": "0.0.1",
  "description": "David's Personal website",
  "scripts": {
    "build:lan": "exec-bin node_modules/.bin/hugo/hugo server --bind=0.0.0.0 --baseURL=http://192.168.1.11 --port=1313",
    "clean": "shx rm -rf public resources",
    "clean:install": "shx rm -rf package-lock.json node_modules",
    "test": "npm run -s lint",
    "postinstall": "hugo-installer --version otherDependencies.hugo --extended --destination node_modules/.bin/hugo"
  },
  "author": "David",
  "license": "MIT",
  "devDependencies": {
    "exec-bin": "^1.0.0",
    "hugo-installer": "^3.1.0",
    "shx": "^0.3.4"
  },
  "otherDependencies": {
    "hugo": "0.96.0"
  }
}
